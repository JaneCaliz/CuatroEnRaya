               <children>
                  <TextField fx:id="usuario" maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="33.0" prefWidth="230.0" promptText="Usuario" style="-fx-background-color: #edf8ff; -fx-border-color: black;">
                     <opaqueInsets>
                        <Insets />
                     </opaqueInsets>
                     <cursor>
                        <Cursor fx:constant="TEXT" />
                     </cursor>
                  </TextField>
                  <PasswordField fx:id="password" maxHeight="-Infinity" maxWidth="-Infinity" prefWidth="230.0" promptText="Contraseña" style="-fx-background-color: #edf8ff; -fx-border-color: black;">
                     <cursor>
                        <Cursor fx:constant="TEXT" />
                     </cursor>
                  </PasswordField>
               </children>
            </VBox>
            <Text fx:id="error" fill="RED" strokeType="OUTSIDE" strokeWidth="0.0" text="Text">
               <font>
                  <Font size="1.0" />
               </font>
               <VBox.margin>
                  <Insets />
               </VBox.margin>
            </Text>
            <Hyperlink fx:id="recuerdame" alignment="TOP_CENTER" focusTraversable="false" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" onAction="#recordar" text="Recordar contraseña" textAlignment="CENTER" textFill="#2d9ab5">
