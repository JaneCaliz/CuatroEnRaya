                  <Insets left="-5.0" top="5.0" />
               </VBox.margin>
            </Hyperlink>
            <Pane maxHeight="20.0" prefHeight="80.0" prefWidth="200.0" VBox.vgrow="ALWAYS" />
            <HBox alignment="CENTER_LEFT" fillHeight="false" prefHeight="37.0" prefWidth="600.0">
               <children>
                  <Button fx:id="registrar" disable="true" focusTraversable="false" mnemonicParsing="false" onAction="#registrar" style="-fx-border-color: #2e7780; -fx-border-radius: 5; -fx-background-radius: 5; -fx-background-color: #52cad9;" text="Regístrate">
