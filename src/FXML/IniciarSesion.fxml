                  <Cursor fx:constant="HAND" />
               </cursor>
            </Button>
         </children>
         <HBox.margin>
            <Insets />
         </HBox.margin>
      </Pane>
      <VBox alignment="CENTER_LEFT" prefHeight="326.0" prefWidth="240.0" HBox.hgrow="ALWAYS">
         <children>
            <Pane fx:id="idk" prefHeight="170.0" prefWidth="200.0" VBox.vgrow="ALWAYS" />
            <Text fx:id="title" focusTraversable="true" strokeType="OUTSIDE" strokeWidth="0.0" text="Inicia sesión" textAlignment="CENTER">
               <font>
                  <Font name="SansSerif Bold" size="25.0" />
               </font>
               <VBox.margin>
                  <Insets bottom="15.0" top="15.0" />
