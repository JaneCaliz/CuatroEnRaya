<HBox alignment="CENTER" prefHeight="300.0" prefWidth="430.0" style="-fx-background-color: b2ebf2;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controladores.IniciarSesionController">
   <children>
