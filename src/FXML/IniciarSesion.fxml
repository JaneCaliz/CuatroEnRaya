                        <Cursor fx:constant="HAND" />
                     </cursor>
                  </Button>
                  <Button fx:id="iniciar" defaultButton="true" mnemonicParsing="false" onAction="#inicio" style="-fx-border-color: #2e7780; -fx-background-radius: 5; -fx-border-radius: 5; -fx-background-color: #52cad9;" text="Iniciar sesión">
